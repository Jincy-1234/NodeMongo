extends layout
block content
 each val in data
  .row
   .col-6
    .card.blue-grey
     .card-content.-text
      .card-title
       span.card-title Saved Data.!
        nav
         ul
          li
           a(href='deleteData?name='+val.name) DELETE
           a(href='update?_id='+val._id) UPDATE
      p Name:#{val.name}
      p Publisher:#{val.phoneNo}
      