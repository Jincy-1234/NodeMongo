
extends layout

block content

 p Registration Form
  each val in data
   form(action='UpdateData' method='get')
    input(type='hidden',name='id',value=val.id)
    .row
     .input-field.col.s6
      label(for='name') Name
      input#name.validate(type='text', name='name')
    .row
     .input-field.col.s6
      label(for='phoneNo') Phone No
      input#publisher.validate(type='text', name='phoneNo') 

    .row
     button.btn(type='Submit') Go








